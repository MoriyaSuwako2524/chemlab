+mat return_x(double t) {
+    mat A(3, 4);
+    A(0,0)=1+t; A(0,1)=cos(t); A(0,2)=0; A(0,3)=pow(t,3);
+    A(1,0)=0; A(1,1)=2-t; A(1,2)=1-t*t; A(1,3)=1.0/t;
+    A(2,0)=exp(t); A(2,1)=t/2; A(2,2)=5; A(2,3)=sin(t);
+    return A;
+}
+
+mat return_dx(double t) {
+    mat dA(3,4);
+    dA(0,0)=1; dA(0,1)=-sin(t); dA(0,2)=0; dA(0,3)=3*t*t;
+    dA(1,0)=0; dA(1,1)=-1; dA(1,2)=-2*t; dA(1,3)=-1.0/(t*t);
+    dA(2,0)=exp(t); dA(2,1)=0.5; dA(2,2)=0; dA(2,3)=cos(t);
+    return dA;
+}
+
