+// ===============================================================
+// ===  Main Test Routine                                       ===
+// ===============================================================
+int main() {
+    double t = 0.5, delta_t = 1e-4;
+    mat A  = return_x(t);
+    mat A1 = return_x(t + delta_t);
+    mat A2 = return_x(t - delta_t);
+    mat dA = return_dx(t);
+
+    cout << "A = \n" << A << endl;
+
+    mat U, V;
+    vec s;
+    svd(U, s, V, A);
+    const uword m = A.n_rows, n = A.n_cols, r = s.n_elem;
+
+    mat U1, V1, U2, V2;
+    vec s1, s2;
+    svd(U1, s1, V1, A1);
+    svd(U2, s2, V2, A2);
+
+    // Align phases between forward/backward finite differences
+    for (uword i = 0; i < r; ++i) {
+        double signU = dot(U1.col(i), U2.col(i)) >= 0 ? 1.0 : -1.0;
+        double signV = dot(V1.col(i), V2.col(i)) >= 0 ? 1.0 : -1.0;
+        U2.col(i) *= signU;
+        V2.col(i) *= signV;
+    }
+
+    // Finite difference derivatives
+    mat U_fd = (U1 - U2) / (2 * delta_t);
+    mat V_fd = (V1 - V2) / (2 * delta_t);
+
+    // Analytical derivatives from operators
+    mat SU = sigma_U_as_operator(U, s, V);
+    mat SV = sigma_V_as_operator(U, s, V);
+
+    vec vec_dA = vectorise(dA);
+    mat pred_dU = reshape(SU * vec_dA, m, r);
+    mat pred_dV = reshape(SV * vec_dA, n, r);
+
+    // Print results
+    cout << "\n=== U_fd ===\n" << U_fd
+         << "\n=== pred_dU ===\n" << pred_dU
+         << "\n\n=== V_fd ===\n" << V_fd
+         << "\n=== pred_dV ===\n" << pred_dV << endl;
+
+    // Compare only first r columns (effective singular subspace)
+    mat V_fd_core = V_fd.cols(0, r - 1);
+
+    return 0;
+}
